<div>
	<div class="main">
		<nav>
			<input id="nav-toggle" type="checkbox" />
			<div class="logo"><img src="/assets/images/finalLogo.png" alt="" /></div>
			<ul class="links">
				<li>
					<a href="/Admin/Home">Dashboard<i class="bx bx-home"></i></a>
				</li>
				<li>
					<a href="/Admin/services">Service
						<i class="bx bx-wrench"></i></a>
				</li>
				<li>
					<a href="/Admin/products">Product
						<i class="bx bx-store-alt"></i></a>
				</li>
				<li>
					<a href="/Admin/request">Request
						<i class="bx bx-envelope"></i></a>
				</li>
				<li>
					<a href="/Admin/Schedules">Schedules<i
							class="bx bx-book-bookmark"
						></i></a>
				</li>
				<li>
					<a href="/Admin/useraccounts">Accounts
						<i class="bx bxs-user-circle"></i></a>
				</li>
				<li>
					<a href="/Admin/archive">Archive
						<i class="bx bxs-archive"></i></a>
				</li>
				<li>
					<a href="/">Log Out
						<i class="bx bx-log-in-circle"></i></a>
				</li>
			</ul>
			<label for="nav-toggle" class="icon-burger">
				<div class="line"></div>
				<div class="line"></div>
				<div class="line"></div>
			</label>
		</nav>

		<div class="Tablemanage">
			<h3 class="fs-4 mb-3">User Accounts</h3>
			<div class="col">
				<div class="table-container">
					<h1 class="tableHeading"></h1>
					<table
						id="data"
						class="table table-striped table-bordered"
						style="width:100%"
					>
						<thead>
							<tr>
								<th>Full Name</th>
								<th>Address</th>
								<th>Contact Number</th>

								<th>Email</th>
							</tr>
						</thead>

						<tbody>
							{{#each resData.data}}
								<tr>
									<td data-lable="Full Name" name="FullName">{{{FullName}}}</td>
									<td data-lable="Address" name="Address">{{{Address}}}</td>
									<td
										data-lable="Contact Number"
										name="ContactNumber"
									>{{{ContactNumber}}}</td>
									<td data-lable="Email" name="Email">{{{Email}}}</td>

								</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
		</div>

	</div>
</div>