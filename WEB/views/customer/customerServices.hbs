<div>
	<nav>
		<input id="nav-toggle" type="checkbox" />
		<div class="logo"><strong>N</strong>orthEast</div>
		<ul class="links">
			<li>
				<a href="/home">Home
					<i class="bx bx-home"></i></a>
			</li>
			<li>
				<a href="/Services">Service
					<i class="bx bx-wrench"></i></a>
			</li>
			<li>
				<a href="/Products">Product
					<i class="bx bx-store-alt"></i></a>
			</li>
			<li>
				<a href="/CART">Cart
					<i class="bx bx-cart"></i></a>
			</li>
			<li>
				<a href="/transactions">Transactions
					<i class="bx bx-book-bookmark"></i></a>
			</li>
			<li>
				<a href="/Notification">Notification
					<i class="bx bx-notification"></i></a>
			</li>
			<li>
				<a href="/Feedback">Feedback
					<i class="bx bx-like"></i></a>
			</li>
			<li>
				<a href="/">Log Out
					<i class="bx bx-log-in-circle"></i></a>
			</li>
		</ul>
		<label for="nav-toggle" class="icon-burger">
			<div class="line"></div>
			<div class="line"></div>
			<div class="line"></div>
		</label>
	</nav>
	<div class="productMain">
		<div class="titleService">
			<h1>Services</h1>
		</div>

		<div class="wrapperGrid">

			{{#each resData.data}}
				<div class="containerServ">
					<h1 class="name">{{{Servicename}}}</h1>
					<div class="imghero">
						<img
							src="{{{Image}}}"
							alt=""
							class="serviceImage"
							style="height: 200px; width:200px"
						/>
					</div>
					<p class="serviceTime">{{{Time}}}</p>
					<form class="addCartFrom" onsubmit="return AddCartBtn(event)">
						<input class="addCartBtn" type="submit" value="Add Cart" />
						<input type="hidden" value="{{{_id}}}" name="ServiceID" />
					</form>
				</div>
			{{/each}}
		</div>
	</div>

</div>