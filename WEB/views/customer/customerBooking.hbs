                    value="{{{_id}}}"
                  >{{{Servicename}}}/{{{Time}}}</option>
                {{/each}} --}}

              {{!-- <div class="row">
                <div class="col">

                  <div class="form-group">
                    <select
                      class="mul-select"
                      name="Services"
                      {{! id="Services" }}
                      id="multiple-checkboxes"
                      multiple="true"
                      required
                    >
                      {{#each resData.data}}
