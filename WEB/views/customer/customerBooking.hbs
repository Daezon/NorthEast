                          value="_id"
                        >{{{Servicename}}}/{{{Time}}}</option>
                      {{/each}}
                    </select>
                  </div>
                </div>
              </div>

              {{! </select> }} --}}

              <label for="Day"><i class="fas fa-calendar-week"></i>
                Date:</label>
              <input type="date" name="Day" id="Day" />

              <label for="Time"><i class="far fa-alarm-clock"></i> Time:</label>
              <select id="Time" name="Time" required>
                <option value="8:00am" name="Time">8:00am</option>
                <option value="9:00am" name="Time">9:00am</option>
                <option value="10:00am" name="Time">10:00am</option>
                <option value="11:00am" name="Time">11:00am</option>
                <option value="1:00pm" name="Time">1:00pm</option>
                <option value="2:00pm" name="Time">2:00pm</option>
                <option value="3:00pm" name="Time">3:00pm</option>
                <option value="4:00pm" name="Time">4:00pm</option>
              </select>

              <label for="RequestType" id="RequestType1"></label>
              <select name="RequestType" id="RequestType2">
                <option value="Default">Default</option>
              </select>

              <input
                type="submit"
                value="Send Request"
                id="bookingBtn"
                class="bookingBtn"
              />

              {{!-- <h1>{{resData.message}}</h1> --}}
